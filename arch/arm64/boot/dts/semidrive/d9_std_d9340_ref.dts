&i2c9 {
//sch part U3(N4)
	status = "okay";

	ov5640:ov5640@3c {
		compatible = "ovti,ov5640";
		clocks = <&CSI_MCLK1>;
		clock-names = "csi-mclk";
		clock-frequency = <12000000>;
		reg = <0x3c>;
		powerdown-gpios = <&port4d 18 GPIO_ACTIVE_HIGH>;//GPIO_G10 - gpio114
		reset-gpios = <&port4d 19 GPIO_ACTIVE_HIGH>;//GPIO_G11 - gpio115

		ports {
			port@0 {
				#address-cells = <1>;
				#size-cells = <0>;

				mcsi0deser_to_mipi_0: endpoint {
					remote-endpoint = <&csimipi0_0_in>;
				};
			};

			port@1 {
				#address-cells = <1>;
				#size-cells = <0>;

				mcsi0deser_to_mipi_1: endpoint {
					remote-endpoint = <&csimipi0_1_in>;
				};
			};
			port@2 {
				#address-cells = <1>;
				#size-cells = <0>;

				mcsi0deser_to_mipi_2: endpoint {
					remote-endpoint = <&csimipi0_2_in>;
				};
			};
			port@3 {
				#address-cells = <1>;
				#size-cells = <0>;

				mcsi0deser_to_mipi_3: endpoint {
					remote-endpoint = <&csimipi0_3_in>;
				};
			};
		};
	};
};		

